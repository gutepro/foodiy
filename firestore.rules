rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
  
    match /recipes/{id} {
      allow read, write: if request.auth != null;
    }

    match /playlists/{id} {
      allow read, write: if request.auth != null;
    }

    // Add similar blocks for any collection that is read during app startup.

  }
}
